  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "babel-loader": "^8.2.3",
    "eslint": "8.4.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-xo-react": "^0.25.0",
